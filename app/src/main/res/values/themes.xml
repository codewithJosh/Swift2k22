<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Swift" parent="Theme.MaterialComponents.Light.NoActionBar.Bridge">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorVividCerulean</item>
        <item name="colorPrimaryVariant">?attr/colorPrimary</item>
        <item name="colorOnPrimary">@color/colorWhite_FF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorStarCommandBlue</item>
        <item name="colorSecondaryVariant">?attr/colorSecondary</item>
        <item name="colorOnSecondary">@color/colorBlack</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>